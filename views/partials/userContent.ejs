<% data.forEach(item=> { %>
    <div class="blog" id="<%= item.id %>">
        <div>
            <h2>
                <%= item.title %>
            </h2>
            <p>
                <%= item.content %>
            </p>
        </div>
        <div class="">
            <p>
                Post at: <%=item.create_at %>
            </p>
            <h3>
                Posted By: <%=item.name %>
            </h3>
        </div>
        <div>
            <a href="/api/blog/<%= item.id %>"><input class="deleteBtn" type="submit" value="Delete"></a>
        </div>
    </div>
    <% }) %>